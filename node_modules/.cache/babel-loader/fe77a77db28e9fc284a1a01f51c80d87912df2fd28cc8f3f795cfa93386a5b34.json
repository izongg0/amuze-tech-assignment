{"ast":null,"code":"import { Disclosure, DisclosureButton, DisclosurePanel } from '@headlessui/vue';\nimport { Bars3Icon, XMarkIcon } from '@heroicons/vue/24/outline';\nimport { reactive } from 'vue';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    // import { reactive } from 'vue'\n\n    const navigation = reactive([{\n      name: '메세지',\n      href: '/message',\n      current: true\n    }, {\n      name: '크레딧',\n      href: '/credit',\n      current: false\n    }]);\n    const setCurrent = item => {\n      navigation.forEach(navItem => {\n        navItem.current = navItem.name === item.name;\n      });\n    };\n    const __returned__ = {\n      navigation,\n      setCurrent,\n      get Disclosure() {\n        return Disclosure;\n      },\n      get DisclosureButton() {\n        return DisclosureButton;\n      },\n      get DisclosurePanel() {\n        return DisclosurePanel;\n      },\n      get Bars3Icon() {\n        return Bars3Icon;\n      },\n      get XMarkIcon() {\n        return XMarkIcon;\n      },\n      reactive\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["Disclosure","DisclosureButton","DisclosurePanel","Bars3Icon","XMarkIcon","reactive","navigation","name","href","current","setCurrent","item","forEach","navItem"],"sources":["/Users/jonghun/Documents/amuze-tech-assignment/src/App.vue"],"sourcesContent":["<!-- <router-link to=\"/\">Home</router-link> |\n<router-link to=\"/message\">Message</router-link> |\n<router-link to=\"/credit\">Credit</router-link> -->\n<template>\n  <Disclosure as=\"nav\" class=\"bg-black h-[80px] \" v-slot=\"{ open }\">\n    <div class=\"mx-auto max-w-7xl px-2 bg-orange sm:px-6 lg:px-8 \">\n      <div class=\"relative flex h-16 items-center justify-between \">\n          <!-- 모바일 -->\n        <div class=\"absolute inset-y-0 left-0 flex items-center sm:hidden\">\n          <DisclosureButton\n            class=\"relative inline-flex items-center justify-center rounded-md p-2 text-gray-400 hover:bg-gray-700 hover:text-white focus:outline-none focus:ring-2 focus:ring-inset focus:ring-white\">\n            <span class=\"absolute -inset-0.5\" />\n            <span class=\"sr-only\">Open main menu</span>\n            <Bars3Icon v-if=\"!open\" class=\"block h-6 w-6\" aria-hidden=\"true\" />\n            <XMarkIcon v-else class=\"block h-6 w-6\" aria-hidden=\"true\" />\n          </DisclosureButton>\n        </div>\n        <!-- 여기까진 모바일 -->\n        <div class=\"flex flex-1 items-center justify-center sm:items-stretch sm:justify-start\">\n          <!-- 여기에 로고 ! -->\n          <div class=\"flex flex-shrink-0 items-center\">\n            <img class=\"h-8 w-auto\" src=\"https://tailwindui.com/img/logos/mark.svg?color=indigo&shade=500\"\n              alt=\"Your Company\" />\n          </div>\n          <div class=\"hidden sm:ml-6 sm:block\">\n            <div class=\"flex space-x-4\">\n              <a v-for=\"item in navigation\" :key=\"item.name\" :href=\"item.href\"\n                :class=\"[item.current ? 'bg-gray-900 text-white' : 'text-gray-300 hover:bg-gray-700 hover:text-white', 'rounded-md px-3 py-2 text-sm font-medium']\"\n                :aria-current=\"item.current ? 'page' : undefined\" @click=\"setCurrent(item)\">{{ item.name }}</a>\n\n              <!-- <a v-for=\"item in navigation\" :key=\"item.name\" :href=\"item.href\" :class=\"[item.current ? 'bg-gray-900 text-white' : 'text-gray-300 hover:bg-gray-700 hover:text-white', 'rounded-md px-3 py-2 text-sm font-medium']\" :aria-current=\"item.current ? 'page' : undefined\">{{ item.name }}</a> -->\n            </div>\n          </div>\n        </div>\n        <div class=\"absolute inset-y-0 right-0 flex items-center pr-2 sm:static sm:inset-auto sm:ml-6 sm:pr-0\">\n\n\n\n        </div>\n      </div>\n    </div>\n\n    <DisclosurePanel class=\"sm:hidden \">\n      <div class=\"space-y-1 px-2 pb-3 pt-2  \">\n        <a v-for=\"item in navigation\" :key=\"item.name\" :href=\"item.href\"\n          :class=\"[item.current ? 'bg-gray-900 text-white' : 'text-gray-300 hover:bg-gray-700 hover:text-white', 'rounded-md px-3 py-2 text-sm font-medium']\"\n          :aria-current=\"item.current ? 'page' : undefined\" @click=\"setCurrent(item)\">{{ item.name }}</a>\n      </div>\n    </DisclosurePanel>\n  </Disclosure>\n  <router-view />\n</template>\n\n\n<script setup>\n\n// import { reactive } from 'vue'\n\nimport { Disclosure, DisclosureButton, DisclosurePanel } from '@headlessui/vue'\nimport { Bars3Icon, XMarkIcon } from '@heroicons/vue/24/outline'\n\n\nimport { reactive } from 'vue'\n\nconst navigation = reactive([\n  { name: '메세지', href: '/message', current: true },\n  { name: '크레딧', href: '/credit', current: false }\n])\n\nconst setCurrent = (item) => {\n  navigation.forEach(navItem => {\n    navItem.current = navItem.name === item.name;\n  });\n  \n}\n\n\n\n\n\n</script>\n\n<style>\n/* #app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n} */\n\n</style>\n"],"mappings":"AA0DA,SAASA,UAAU,EAAEC,gBAAgB,EAAEC,eAAe,QAAQ,iBAAiB;AAC/E,SAASC,SAAS,EAAEC,SAAS,QAAQ,2BAA2B;AAGhE,SAASC,QAAQ,QAAQ,KAAK;;;;;;;;IAN9B;;IAQA,MAAMC,UAAU,GAAGD,QAAQ,CAAC,CAC1B;MAAEE,IAAI,EAAE,KAAK;MAAEC,IAAI,EAAE,UAAU;MAAEC,OAAO,EAAE;IAAK,CAAC,EAChD;MAAEF,IAAI,EAAE,KAAK;MAAEC,IAAI,EAAE,SAAS;MAAEC,OAAO,EAAE;IAAM,CAAC,CACjD,CAAC;IAEF,MAAMC,UAAU,GAAIC,IAAI,IAAK;MAC3BL,UAAU,CAACM,OAAO,CAACC,OAAO,IAAI;QAC5BA,OAAO,CAACJ,OAAO,GAAGI,OAAO,CAACN,IAAI,KAAKI,IAAI,CAACJ,IAAI;MAC9C,CAAC,CAAC;IAEJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}